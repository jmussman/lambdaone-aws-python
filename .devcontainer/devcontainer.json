{
    "name": "LambdaOne AWS Python",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.debugpy"
            ]        
        }
    },
    "forwardPorts": [
        8000,
        9000
    ],
    "portsAttributes": {
        "8800": {
            "label": "jwks"
        },
        "9000": {
            "label": "docker"
        }
    },
    "postAttachCommand": ". ./.devcontainer/postAttachCommand.sh"
}